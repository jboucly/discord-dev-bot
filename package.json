{
	"name": "discord-daily-bot",
	"version": "0.0.6",
	"description": "Discord bot for weekly daily",
	"main": "dist/index.js",
	"repository": "git@github.com:jboucly/discord-daily-bot.git",
	"author": "jboucly <julien.boucly@outlook.com>",
	"license": "MIT",
	"engines": {
		"node": "22.11.0"
	},
	"scripts": {
		"heroku-postbuild": "yarn build && mkdir ./.cache && mv /app/.cache/puppeteer ./.cache",
		"dev": "nodemon",
		"start": "node dist/index.js",
		"build": "rimraf dist && tsc",
		"deploy": "yarn build && node dist/deploy-commands.js",
		"prettier": "prettier --config ./.prettierrc --check \"src/**/*.ts\"",
		"prettier:fix": "prettier --config ./.prettierrc --write \"src/**/*.ts\"",
		"prisma": "prisma",
		"db:generate": "prisma generate",
		"db:deploy": "prisma migrate deploy",
		"db:format": "prisma format",
		"db:migrate:reset": "prisma migrate reset",
		"db:studio": "prisma studio",
		"db:validate": "prisma validate",
		"db:migrate:create": "prisma migrate dev --create-only",
		"db:migrate:dev": "prisma migrate dev"
	},
	"dependencies": {
		"@prisma/client": "6.1.0",
		"cron": "^3.3.2",
		"cronstrue": "^2.32.0",
		"date-fns": "^4.1.0",
		"discord.js": "14.17.2",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"lodash": "^4.17.21",
		"module-alias": "^2.2.3",
		"node-cron": "^3.0.2",
		"node-fetch": "2",
		"prettier": "^3.3.3",
		"prisma": "^6.1.0",
		"puppeteer-extra": "^3.3.6",
		"puppeteer-extra-plugin-adblocker": "^2.13.6",
		"puppeteer-extra-plugin-stealth": "^2.11.2",
		"rimraf": "^6.0.1",
		"typescript": "^5.7.3"
	},
	"devDependencies": {
		"@types/cron": "^2.0.1",
		"@types/express": "^5.0.0",
		"@types/lodash": "^4.14.191",
		"@types/node-cron": "^3.0.7",
		"@types/node-fetch": "^2.6.6",
		"@types/puppeteer": "^7.0.4",
		"nodemon": "^3.1.9",
		"ts-node": "^10.9.2"
	},
	"_moduleAliases": {
		"@common": "./dist/common",
		"@events": "./dist/events",
		"@commands": "./dist/commands"
	}
}
